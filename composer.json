{
	"name": "aerni/snipcart-api",
	"description": "A Laravel wrapper for the Snipcart API",
	"keywords": [
		"laravel",
		"snipcart",
		"snipcart-api",
		"wrapper",
		"api",
		"e-commerce",
		"shop"
	],
	"homepage": "https://github.com/aerni/snipcart-api",
	"license": "MIT",
	"authors": [
		{
			"name": "Michael Aerni",
			"email": "hello@michaelaerni.ch",
			"homepage": "https://www.michaelaerni.ch",
			"role": "Developer"
		}
	],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.0|^6.1",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
	"autoload": {
		"psr-4": {
			"Aerni\\SnipcartApi\\": "src"
		}
    },
    "autoload-dev": {
        "psr-4": {
            "Aerni\\SnipcartApi\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Aerni\\SnipcartApi\\SnipcartApiServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
